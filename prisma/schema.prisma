model CreatorRegistration {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  role      String   // "creator" or "clipper"
  youtube   String?
  twitch    String?
  instagram String?
  tiktok    String?
  createdAt DateTime @default(now())
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
} 